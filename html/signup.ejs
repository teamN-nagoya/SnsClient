<html lang="jp">    
    <head>
        <meta charset="UTF-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>サインアップ</title>
        <link rel="stylesheet" href="https://use.typekit.net/ioq0uqc.css">
        <link rel="stylesheet" href=".././style/main.css">
    </head>
    <body>
        <h1>サインアップ</h1>
        <hr>
        <div align="center">
            <table border="0">
                <tr>
                    <th>
                        ユーザID
                    </th>
                    <td>
                        <input id="user_id" type="text" name="user_id" value="" size="24">
                    </td>
                </tr>
                <tr>
                    <th>
                        ユーザネーム
                    </th>
                    <td>
                        <input id="user_name" type="text" name="user_name" value="" size="24">
                    </td>
                </tr>
                <tr>
                    <th>
                        パスワード
                    </th>
                    <td>
                        <input id="password" type="password" name="password" value="" size="24">
                    </td>
                </tr>
                <tr>
                    <td colspan="2">
                        <input id="submit" type="submit" value="サインアップ">
                    </td>
                    <label id="error">XXX</label>
                </tr>
            </table>
        </div>
    </body>
</html>